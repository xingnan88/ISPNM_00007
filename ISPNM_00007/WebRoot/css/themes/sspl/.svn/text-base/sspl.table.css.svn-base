.table {
	text-align: left;
	cursor: pointer;
	margin: auto;
	width: 100%;
	border-top: 1px solid #999;
	border-left: 1px solid #999;
}

.table tr {
	
}

.table th {
	background: url("images/button.png") repeat-x;
	height: 28px;
	border-right: 1px solid #999;
	border-bottom: 1px solid #999;
}

.table td {
	border-right: 1px solid #999;
	border-bottom: 1px solid #999;
	padding: 2px 5px 2px 7px;
}

.table tr:hover {
	background: url("images/button.png") repeat-x;
}